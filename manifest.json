{
  "manifest_version": 2,
  "name": "DLsiteWarn",
  "version": "0.0.1",
  "description": "DLサイト閲覧時に要注意サークルを表示する",
  "content_scripts": [
    {
      "matches": ["http://www.dlsite.com/*"],
      "js": ["js/jquery-3.1.1.min.js", "js/warncircle.js"]
    }
  ],
  "background": {
    "scripts": ["js/load_circles.js"],
    "persistent": false
  }
}
